{
  "name": "noonlight-stl-full",
  "version": "1.0.0",
  "description": "Data visualization for Noonlight app usage in St. Louis with KeplerGL",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "client": "webpack-dev-server --config webpack.dev.js --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "npm run build"
  },
  "author": "Michael Martin",
  "license": "MIT",
  "engines": {
    "node": "9.5.0"
  },
  "dependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "dotenv-webpack": "^1.5.7",
    "express": "^4.16.3",
    "global": "^4.3.2",
    "html-webpack-plugin": "^3.2.0",
    "kepler.gl": "^0.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-palm": "^0.8.1",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "webpack-merge": "^4.1.4",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
